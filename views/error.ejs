<% layout('layouts/boilerplate') %>

    <div class="row">
        <div class="col-6 offset-3">
            <div class="alert alert-warning" role="alert">
                <h4 class="alert-heading">ERROR!</h4>
                <hr>
                <h4> <%=err.message%></h4>
                <hr>
               
            <% if(process.env.NODE_ENV!=="production"){ %>  
                <p> <%= err.stack %></p>
            <% } %>    
                
            </div>
        </div>
    </div>
    
    